{
    "name": "rybakit/arguments-resolver",
    "description": "ArgumentsResolver allows you to determine the arguments to pass to a function or method.",
    "keywords": ["arguments", "parameters", "function", "method", "callable", "reflection", "injection", "invoke", "resolver"],
    "homepage": "https://github.com/rybakit/arguments-resolver",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Eugene Leonovich",
            "email": "gen.work@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1|^8"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.13",
        "phpunit/phpunit": "^7.1|^8|^9"
    },
    "autoload": {
        "psr-4": {
            "ArgumentsResolver\\": "src/"
        }
    },
    "autoload-dev" : {
        "psr-4": {
            "ArgumentsResolver\\Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    }
}
